<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="460.0" prefWidth="600.0" style="-fx-background-color: #2C2C45;" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.Authentication">
   <children>
      <Button fx:id="join" layoutX="150.0" layoutY="140.0" mnemonicParsing="false" onAction="#join" prefHeight="70.0" prefWidth="300.0" style="-fx-background-color: #575789;" text="Join" textFill="WHITE">
         <font>
            <Font name="Futura Medium" size="34.0" />
         </font></Button>
      <Button fx:id="registration" layoutX="150.0" layoutY="250.0" mnemonicParsing="false" onAction="#registration" prefHeight="70.0" prefWidth="300.0" style="-fx-background-color: #575789;" text="Registration" textFill="WHITE">
         <font>
            <Font name="Futura Medium" size="34.0" />
         </font></Button>
      <Label layoutX="130.0" layoutY="34.0" text="Press the button!" textFill="#6e7fd3">
         <font>
            <Font name="American Typewriter" size="41.0" />
         </font>
      </Label>
      <ImageView fitHeight="48.0" fitWidth="48.0" layoutX="552.0" layoutY="1.0" onMouseClicked="#closeWindow" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@x.png" />
         </image></ImageView>
   </children>
</AnchorPane>
